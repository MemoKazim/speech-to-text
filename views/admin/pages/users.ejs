<%if (!ajax){%>
  <%-include("../partials/header", {title: "Dashboard"})-%>
  <body>
      <div class="sidebar" data-theme="light">
          <%-include("../partials/sidebar")-%>
      </div>
      <div class="content-home" id="content-home" data-theme="light">
<%}%>
    <div class="page-header">
      <%-page_name-%>
      <!-- <button class="add-button" onclick="getNewReport()">
        <i class="fa-solid fa-plus"></i>
      </button> -->
    </div>
      <div class="table-container">
        <table class="rounded-table">
          <thead>
            <tr>
              <th>#</th>
              <th>Username</th>
              <th>Email</th>
              <th>Action</th>
            </tr>
          </thead>
          <tbody>
            <%data.forEach( (item, i) => {%>
              <tr>

                <td> <%-i-%> </td>
                <td> <%-item.username-%> </td>
                <td> <%-item.email-%> </td>
                <td> <table class="action">
                  <tr>
                    <td><a href="/admin/users/<%=item._id%>"><i class="fa-solid fa-download" style="color: #54b5ff;"></i></a></td>
                    <td><a href="/admin/users/delete/<%=item._id%>"><i class="fa-solid fa-trash" style="color:red"></i></a></td>
                  </tr>
                </table> </td>
              </tr>
            <%})%>
          </tbody>
        </table>
      </div>
<%if (!ajax){%>
      </div>
  <%-include("../partials/footer")-%>
<%}%>
